void main() {
  
  List <int> numeros = [1,3,2,6,19,16,14,15];
  somaImpares(numeros);
  
}

void somaImpares(List<int> numeros){
  int soma = 0;
  int i = 0;
    
  while(i < numeros.length){
  
    if(numeros[i] % 2 != 0){
      soma += numeros[i];
    }
    i++;
  }
  print("O total das somas dos numeros impares Ã© ${soma}");
}
